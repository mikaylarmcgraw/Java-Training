//Task 1.7 do while loop exercise

//Execute a do while statement for integer values 2 through and including 8.

//Display each value from within the loop  

public class Task1_7

{

  public static void main (String args[])

   {

    // no input error trapping necessary on main arguments

    int i = 2;

  

    while (i<=8)

     {

         System.out.println(i);

         i++;

     }

   }

  

 }
